  <title>StockWasp | Realtime Stock Prices and News</title>
  <link rel="stylesheet" href="/bootstrap.min.css" />
  <script src="/bootstrap.min.js"></script>

  <meta name="description"
    content="StockWasp provides a simple and clean interface to find all your favorite stocks price and news information" />
  <meta name="robots" content="index, follow" />
  <link rel="canonical" href="https://stockwasp.com" />
  <meta property="og:image" content="https://stockwasp.com/opengraph_logo.svg" />
  <meta property="og:site_name" content="StockWasp" />
  <meta property="og:title" content="StockWasp" />
  <meta property="og:description" content="StockWasp provides realtime stock data along with simple charts" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="https://stockwasp.com" />
