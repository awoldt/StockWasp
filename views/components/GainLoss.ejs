<span style="display: inline-block;">
  <% if (data !==null && data.companyProfile !==null) { %>
    <span>
      <br>
      <% if (data.companyQuote.change !==null && data.companyQuote.percent_change < 0) { %>
        <span style="color: red;">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-down"
            viewBox="0 0 16 16">
            <path fill-rule="evenodd"
              d="M8 1a.5.5 0 0 1 .5.5v11.793l3.146-3.147a.5.5 0 0 1 .708.708l-4 4a.5.5 0 0 1-.708 0l-4-4a.5.5 0 0 1 .708-.708L7.5 13.293V1.5A.5.5 0 0 1 8 1z" />
          </svg>
          <%= data.companyQuote.change.toFixed(2) %> (<%= data.companyQuote.percent_change.toFixed(2) %>%)

        </span>
        <% } else if(data.companyQuote.change !==null && data.companyQuote.percent_change> 0) { %>
          <span style="color: green;">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-up"
              viewBox="0 0 16 16">
              <path fill-rule="evenodd"
                d="M8 15a.5.5 0 0 0 .5-.5V2.707l3.146 3.147a.5.5 0 0 0 .708-.708l-4-4a.5.5 0 0 0-.708 0l-4 4a.5.5 0 1 0 .708.708L7.5 2.707V14.5a.5.5 0 0 0 .5.5z" />
            </svg>
            <%= data.companyQuote.change.toFixed(2) %> (<%= data.companyQuote.percent_change.toFixed(2) %>%)

          </span>
          <% } else { %>
            <span style="color: black;">
              <%= data.companyQuote.change.toFixed(2) %> (<%= data.companyQuote.percent_change.toFixed(2) %>%)
            </span>
            <% } %>
              <% } %>

    </span>
</span>